{
    "manifest_version": 3,
    "name": "URL Modifier",
    "version": "1.0",
    "description": "Modifies URLs with parameters and environments",
    "permissions": [
      "activeTab",
      "storage",
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }